include "include/map.inc"
include "include/marker.inc"
include "include/pioneer.inc"

window
(
    size [800 600]
    show_data 1
    #show_ranger_beams 1
)

floorplan
(
    bitmap "bitmaps/kitchen.png"
    size [10 6 1]
)

